{
  "expo": {
    "name": "premKing",
    "slug": "premking",
    "scheme": "premKing",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/IOS-LOGO-3x.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/premKingLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#072936"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kristo.premking",
      "name": "pk-app",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/IOS-LOGO-3x.png",
        "backgroundColor": "#072936"
      },
      "package": "com.kristo.premking"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/IOS-LOGO-3x.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d7fc379a-c178-4bfa-8fc4-e91110513d54"
      }
    },
    "owner": "kristoferfannar"
  }
}
